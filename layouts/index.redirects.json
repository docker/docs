{{ $redirects := newScratch }}
{{- range $i, $e := site.AllPages -}} 
  {{- if .Params.aliases -}}
    {{- $target := .RelPermalink -}}
    {{- range .Params.aliases -}}
      {{ $redirects.SetInMap "paths" . $target }}
    {{- end -}}
  {{- end -}}
{{- end -}}
{{- range $target, $aliases := site.Data.redirects -}}
{{- range $aliases }}
{{- $redirects.SetInMap "paths" . $target -}}
{{- end -}}
{{- end -}}
{{ $redirects.Get "paths" | jsonify }}
