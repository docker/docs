#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/GitHub/docker/node_modules/.pnpm/pnpm@6.14.3/node_modules/pnpm/bin/node_modules:/mnt/c/GitHub/docker/node_modules/.pnpm/pnpm@6.14.3/node_modules/pnpm/node_modules:/mnt/c/GitHub/docker/node_modules/.pnpm/pnpm@6.14.3/node_modules:/mnt/c/GitHub/docker/node_modules/.pnpm/node_modules:/mnt/c/GitHub/docker/node_modules:/mnt/c/GitHub/node_modules:/mnt/c/node_modules:/mnt/c/GitHub/docker/node_modules/pnpm/bin/node_modules:/mnt/c/GitHub/docker/node_modules/pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/GitHub/docker/node_modules/.pnpm/pnpm@6.14.3/node_modules/pnpm/bin/node_modules:/mnt/c/GitHub/docker/node_modules/.pnpm/pnpm@6.14.3/node_modules/pnpm/node_modules:/mnt/c/GitHub/docker/node_modules/.pnpm/pnpm@6.14.3/node_modules:/mnt/c/GitHub/docker/node_modules/.pnpm/node_modules:/mnt/c/GitHub/docker/node_modules:/mnt/c/GitHub/node_modules:/mnt/c/node_modules:/mnt/c/GitHub/docker/node_modules/pnpm/bin/node_modules:/mnt/c/GitHub/docker/node_modules/pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pnpm/bin/pnpm.cjs" "$@"
else
  exec node  "$basedir/../pnpm/bin/pnpm.cjs" "$@"
fi
